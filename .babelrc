{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": 6,
        "browsers": [
          "> 0.5%",
          "last 2 versions"
        ]
      }
    }],
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "helpers": true,
      "regenerator": true,
    }]
  ],
  "sourceType": "unambiguous"
}
